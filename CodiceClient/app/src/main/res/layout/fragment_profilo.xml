<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Views.ProfiloFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F5EDED">

        <TextView
            android:id="@+id/promptDettagliRistorante"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="572dp"
            android:layout_marginEnd="563dp"
            android:fontFamily="@font/verdana_font"
            android:text="I tuoi dettagli:"
            android:textColor="@color/colorGrey"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.44"
            app:layout_constraintVertical_chainStyle="packed" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout8"
            android:layout_width="867dp"
            android:layout_height="353dp"
            android:layout_marginStart="186dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="185dp"
            android:layout_marginBottom="80dp"
            android:background="@drawable/dummy_for_shadows_rounded"
            android:elevation="5dp"
            app:layout_constraintBottom_toTopOf="@+id/bottoneAnnullaModificaRistorante"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/promptDettagliRistorante"
            app:layout_constraintVertical_bias="0.51">

            <TextView
                android:id="@+id/textViewNomeRistorantePromptModifica"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Nome:"
                android:textColor="@color/colorGrey"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.132"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.215" />

            <TextView
                android:id="@+id/textViewNumeroRistorantePromptModifica"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:text="Ruolo:"
                android:textColor="@color/colorGrey"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/textViewNomeRistorantePromptModifica"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textViewNomeRistorantePromptModifica"
                app:layout_constraintTop_toBottomOf="@+id/textViewNomeRistorantePromptModifica"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textViewIndirizzoRistorantePromptModifica"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:text="E-mail:"
                android:textColor="@color/colorGrey"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/textViewNumeroRistorantePromptModifica"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textViewNumeroRistorantePromptModifica"
                app:layout_constraintTop_toBottomOf="@+id/textViewNumeroRistorantePromptModifica"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/textViewTuristicoRistoranteModifica"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:text="Password:"
                android:textColor="@color/colorGrey"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/textViewIndirizzoRistorantePromptModifica"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/textViewIndirizzoRistorantePromptModifica"
                app:layout_constraintTop_toBottomOf="@+id/textViewIndirizzoRistorantePromptModifica"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/iconaNomeRistorante"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginEnd="7dp"
                android:src="@drawable/icon_credenziali"
                app:layout_constraintBottom_toBottomOf="@+id/textViewNomeRistorantePromptModifica"
                app:layout_constraintEnd_toStartOf="@+id/textViewNomeRistorantePromptModifica"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textViewNomeRistorantePromptModifica"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/iconaNumeroRistorante"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginEnd="7dp"
                android:src="@drawable/icon_ruolo"
                app:layout_constraintBottom_toBottomOf="@+id/textViewNumeroRistorantePromptModifica"
                app:layout_constraintEnd_toStartOf="@+id/textViewNumeroRistorantePromptModifica"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textViewNumeroRistorantePromptModifica" />

            <ImageView
                android:id="@+id/iconaIndirizzoRistorante"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginEnd="7dp"
                android:src="@drawable/icon_profile"
                app:layout_constraintBottom_toBottomOf="@+id/textViewIndirizzoRistorantePromptModifica"
                app:layout_constraintEnd_toStartOf="@+id/textViewIndirizzoRistorantePromptModifica"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textViewIndirizzoRistorantePromptModifica" />

            <ImageView
                android:id="@+id/iconaRistoranteTuristico"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginEnd="7dp"
                android:src="@drawable/icon_password"
                app:layout_constraintBottom_toBottomOf="@+id/textViewTuristicoRistoranteModifica"
                app:layout_constraintEnd_toStartOf="@+id/textViewTuristicoRistoranteModifica"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textViewTuristicoRistoranteModifica" />

            <ImageView
                android:id="@+id/iconaModificaPasswordProfilo"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_marginStart="20dp"
                android:src="@drawable/icona_matita"
                app:layout_constraintBottom_toBottomOf="@+id/textViewTuristicoRistoranteModifica"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/textViewTuristicoRistoranteModifica"
                app:layout_constraintTop_toTopOf="@+id/textViewTuristicoRistoranteModifica" />

            <TextView
                android:id="@+id/textViewIndirizzoRistoranteVisualizza"
                android:layout_width="wrap_content"
                android:layout_height="22dp"
                android:layout_marginStart="7dp"
                android:layout_marginEnd="7dp"
                android:background="@android:color/transparent"
                android:inputType="textEmailAddress"
                android:backgroundTint="#5F5959"
                android:maxWidth="600dp"
                android:singleLine="true"
                android:text="TextView"
                android:textCursorDrawable="@drawable/color_cursor"
                android:textColor="@color/colorGrey"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="@+id/textViewIndirizzoRistorantePromptModifica"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/textViewIndirizzoRistorantePromptModifica"
                app:layout_constraintTop_toTopOf="@+id/textViewIndirizzoRistorantePromptModifica" />

            <TextView
                android:id="@+id/textViewNumeroRistoranteVisualizza"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginStart="7dp"
                android:text="TextView"
                android:textColor="@color/colorGrey"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="@+id/textViewNumeroRistorantePromptModifica"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/textViewNumeroRistorantePromptModifica"
                app:layout_constraintTop_toTopOf="@+id/textViewNumeroRistorantePromptModifica" />

            <TextView
                android:id="@+id/textViewNomeRistoranteVisualizza"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginStart="7dp"
                android:text="TextView"
                android:textColor="@color/colorGrey"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="@+id/textViewNomeRistorantePromptModifica"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toEndOf="@+id/textViewNomeRistorantePromptModifica"
                app:layout_constraintTop_toTopOf="@+id/textViewNomeRistorantePromptModifica" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/bottoneAnnullaModificaRistorante"
            android:layout_width="118dp"
            android:layout_height="48dp"
            android:background="@drawable/login_box_border"
            android:backgroundTint="#FF0000"
            android:fontFamily="@font/verdana_font"
            android:text="Logout"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout8"
            app:layout_constraintVertical_bias="0.875" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="466dp"
            android:layout_height="431dp"
            android:layout_marginEnd="500dp"
            android:layout_marginBottom="1dp"
            android:src="@drawable/impostazioni"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/bottoneAnnullaModificaRistorante"
            app:layout_constraintHorizontal_bias="0.163"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout8"
            app:layout_constraintVertical_bias="0.363" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>